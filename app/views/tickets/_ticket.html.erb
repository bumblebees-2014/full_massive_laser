<div class="container">
  <div class="clearfix">
    <div class="col col-12 p1">
      <div class="overflow-hidden bg-white border rounded">

        <div class="p2 bg-yellow">
          <h1 class="h2 m0">
            Claimed Tickets
          </h1>
        </div>

        <div class="overflow-scroll">
        <table class="table-light">
        <thead>
          <tr>
            <th>User</th> <th>Category</th> <th>Title</th>
          </tr>
        </thead>
        <tbody>
          <% @tickets.each do |ticket| %>
          <tr>
            <td><%= ticket.user.name %></td>
            <td><%= ticket.profession.name %></td>
            <td><%= ticket.title %></td>
          </tr>
          <% end %>
        </tbody>
        </table>
        </div>

      </div>
    </div>
  </div>
</div>