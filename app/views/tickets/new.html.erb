<%= form_for @ticket, :html => { :class => "sm-col-6"} do |f| %>
<fieldset class="fieldset-reset">
  <legend class="h3 bold">New Ticket</legend>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>

  <%= f.label :body %><br>
  <%= f.text_area :body %><br>




</fieldset>

  <%= f.submit %>
<% end %>
