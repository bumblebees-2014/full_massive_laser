<%= render 'login_form' %>

    <div>
      <% if current_user %>
        Signed in as <strong><%= current_user.name %></strong>!
        <%= link_to "Sign out", signout_path, id: "sign_out" %>
      <% else %>
<button class="google-signin nav-block"><%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in" %></button>
      <% end %>
    </div>